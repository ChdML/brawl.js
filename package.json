{
    "version": "0.1.7",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "type": "commonjs",
    "keywords": ["brawlhalla", "api", "brawlhalla-api", "brawlhalla-api-client", "bh", "brawlhallaapi"],
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "repository": {"url": "https://github.com/ChdML/brawl.js/"},
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "brawl.js",
    "author": "ChdML",
    "module": "dist/brawlhallaapi.esm.js",
    "size-limit": [
        {
            "path": "dist/brawlhallaapi.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/brawlhallaapi.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "husky": "^7.0.4",
        "size-limit": "^7.0.8",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "axios": "^0.27.2"
    }
}
